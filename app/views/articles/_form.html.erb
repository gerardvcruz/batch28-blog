<%= form_with scope: :article, url: create_article_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :body %><br>
    <%= f.text_field :body %>
  </p>


  <p>
    <%= f.label :id %><br>
    <%= f.text_field :id %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
